<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/avatarBg">
        <shape android:shape="rectangle">
            <solid android:color="@color/btnCustom" android:alpha="0.8"/>
            <size android:height="24dp" android:width="90dp"/>
        </shape>
    </item>
    <item
        android:id="@+id/icon"
